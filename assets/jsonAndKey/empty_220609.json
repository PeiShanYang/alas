{
    "Config": {
        "PrivateSetting": {
            "datasetPath": {
                "type": "string"
            }
        }
    },
    "ConfigAugmentation": {
        "AugmentationPara": {
            "randomHorizontalFlip": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "probability": {
                    "type": "flaot",
                    "default": 0.5,
                    "max": 1,
	                "min": 0
                }
            },
            "randomVerticalFlip": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "probability": {
                    "type": "flaot",
                    "default": 0.5,
                    "max": 1,
	                "min": 0
                }
            },
            "randomRotation": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "degrees": {
                    "type": "list[float]",
                    "default": [-360, 360],
                    "max": 360,
                    "min": -360,
                    "len": 2,
                    "orderable": "True"
                }
            },
            "randomTranslate": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "translate": {
                    "type": "list[float]",
                    "default": [0, 0],
                    "max": 1,
                    "min": 0,
                    "len": 2
                }
            },
            "randomScale": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "scale": {
                    "type": "list[float]",
                    "default": [1, 1],
                    "min": 0,
                    "len": 2,
                    "orderable": "True"
                }
            },
            "randomShear": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "shear": {
                    "type": "list[float]",
                    "default": [0, 0, 0, 0],
                    "max": 360,
                    "min": -360,
                    "len": 4,
                    "orderable_2": "True"
                }
            },
            "randomGrayscale": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "probability": {
                    "type": "flaot",
                    "default": 0.5,
                    "max": 1,
	                "min": 0
                }
            },
            "randomBrightness": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "brightness": {
                    "type": "list[float]",
                    "default": [1, 1],
                    "min": 0,
                    "len": 2,
                    "orderable": "True"
                }
            },
            "randomContrast": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "contrast": {
                    "type": "list[float]",
                    "default": [1, 1],
                    "min": 0,
                    "len": 2,
                    "orderable": "True"
                }
            },
            "randomSaturation": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "saturation": {
                    "type": "list[float]",
                    "default": [1, 1],
                    "min": 0,
                    "len": 2,
                    "orderable": "True"
                }
            },
            "randomHue": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "hue": {
                    "type": "list[float]",
                    "default": [0, 0],
                    "max": 0.5,
                    "min": -0.5,
                    "len": 2,
                    "orderable": "True"
                }
            },
            "randomErasing": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "probability": {
                    "type": "flaot",
                    "default": 0.5,
                    "max": 1,
	                "min": 0
                },
                "scale": {
                    "type": "list[float]",
                    "default": [0.02, 0.33],
                    "max": 1,
                    "min": 0,
                    "len": 2,
                    "orderable": "True"
                },
                "ratio": {
                    "type": "list[float]",
                    "default": [0.3, 3.3],
                    "min": 0,
                    "len": 2,
                    "orderable": "True"
                },
                "value": {
                    "type": "list[int]",
                    "default": [0, 0, 0],
                    "max": 255,
                    "min": 0,
                    "len": 3
                }
            },
            "randomPerspective": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "distortion" : {
                    "type": "float",
                    "default": 0,
                    "max": 1,
                    "min": 0
                },
                "probability": {
                    "type": "flaot",
                    "default": 0.5,
                    "max": 1,
	                "min": 0
                },
                "interpolation": {
                    "type": "string",
                    "default": "BILINEAR",
                    "enums": "BILINEAR, NEAREST, BICUBIC"
                },
                "fill": {
                    "type": "list[int]",
                    "default": [0, 0, 0],
                    "max": 255,
                    "min": 0,
                    "len": 3
                }
            }
        }
    },
    "ConfigEvaluation": {
        "EvaluationPara": {
            "showAcc": {
                "switch": {
                    "type": "boolean",
                    "default": "True"
                }
            },
            "showClassAcc": {
                "switch": {
                    "type": "boolean",
                    "default": "True"
                }
            },
            "showNumOfClasses": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                }
            },
            "showWrongFile": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                }
            }
        }
    },
    "ConfigModelService": {
        "LossFunctionPara": {
            "lossFunction": {
                "type": "string",
                "default": "CrossEntropyLoss",
                "enums": "CrossEntropyLoss, MSELoss, L1Loss, SmoothL1Loss, BCELoss"
            }
        },
        "LearningRate": {
            "learningRate": {
                "type": "float",
                "default": 0.001,
                "min": 0
            }
        },
        "OptimizerPara": {
            "SGD": {
                "switch": {
                    "type": "boolean",
                    "default": "True"
                },
                "momentum": {
                    "type": "float",
                    "default": 0.9,
                    "max": 1,
                    "min": 0
                },
                "dampening": {
                    "type": "float",
                    "default": 0,
                    "max": 1,
                    "min": 0
                },
                "weightDecay": {
                    "type": "float",
                    "default": 5e-4,
                    "max": 1,
                    "min": 0
                },
                "nesterov": {
                    "type": "boolean",
                    "default": "False"
                }
            },
            "Adam": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "betas": {
                    "type": "list[float]",
                    "default": [0.9, 0.999],
                    "max": 1,
                    "min": 0,
                    "len": 2
                },
                "eps": {
                    "type": "float",
                    "default": 1e-8,
                    "max": 1,
                    "min": 0
                },
                "weightDecay": {
                    "type": "float",
                    "default": 0,
                    "max": 1,
                    "min": 0
                },
                "amsgrad": {
                    "type": "boolean",
                    "default": "False"
                }
            },
            "Adadelta": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "rho":  {
                    "type": "float",
                    "default": 0.9,
                    "max": 1,
                    "min": 0
                },
                "eps": {
                    "type": "float",
                    "default": 1e-6,
                    "max": 1,
                    "min": 0
                },
                "weightDecay": {
                    "type": "float",
                    "default": 0,
                    "max": 1,
                    "min": 0
                }
            },
            "AdamW": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "betas": {
                    "type": "list[float]",
                    "default": [0.9, 0.999],
                    "max": 1,
                    "min": 0,
                    "len": 2
                },
                "eps": {
                    "type": "float",
                    "default": 1e-8,
                    "max": 1,
                    "min": 0
                },
                "weightDecay": {
                    "type": "float",
                    "default": 0.01,
                    "max": 1,
                    "min": 0
                },
                "amsgrad": {
                    "type": "boolean",
                    "default": "False"
                }
            },
            "NAdam": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "betas": {
                    "type": "list[float]",
                    "default": [0.9, 0.999],
                    "max": 1,
                    "min": 0,
                    "len": 2
                },
                "eps": {
                    "type": "float",
                    "default": 1e-8,
                    "max": 1,
                    "min": 0
                },
                "weightDecay": {
                    "type": "float",
                    "default": 0.01,
                    "max": 1,
                    "min": 0
                },
                "momentumDecay": {
                    "type": "float",
                    "default": 0.004,
                    "max": 1,
                    "min": 0
                }
            }        
        },
        "SchedulerPara": {
            "stepLR": {
                "switch": {
                    "type": "boolean",
                    "default": "True"
                },
                "stepSize": {
                    "type": "int",
                    "default": 10,
                    "min": 1
                },
                "gamma": {
                    "type": "float",
                    "default": 0.1
                }
            },
            "cosineAnnealingLR": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "tMax": {
                    "type": "int",
                    "min": 1
                },
                "etaMin": {
                    "type": "float",
                    "default": 0,
                    "min": 0
                }
            }
        }
    },
    "ConfigPreprocess": {
        "PreprocessPara": {
            "normalize": {
                "switch": {
                    "type": "boolean",
                    "default": "True"
                },
                "mode": {
                    "type": "int",
                    "default": 0,
                    "enums": "0, 1, 2, 3, 4"
                },
                "mean": {
                    "type": "list[float]",
                    "default": [0.5, 0.5, 0.5],
                    "min": 0,
                    "len": 3,
                    "nullable": "True"
                },
                "std": {
                    "type": "list[float]",
                    "default": [0.5, 0.5, 0.5],
                    "min": 0,
                    "len": 3,
                    "nullable": "True"
                }
            },
            "resize": {
                "switch": {
                    "type": "boolean",
                    "default": "True"
                },
                "imageSize": {
                    "type": "list[int]",
                    "default": [224, 224],
                    "min": 1,
                    "len": 2
                },
                "interpolation": {
                    "type": "string",
                    "default": "BILINEAR",
                    "enums": "BILINEAR, NEAREST, BICUBIC"
                }
            },
            "centerCrop": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "size": {
                    "type": "list[int]",
                    "default": [1, 1],
                    "min": 0,
                    "len": 2
                }
            },
            "pad": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "padding":  {
                    "type": "list[int]",
                    "default": [0, 0, 0, 0],
                    "min": 0,
                    "len": 4
                },
                "fill": {
                    "type": "list[int]",
                    "default": [0, 0, 0],
                    "max": 255,
                    "min": 0,
                    "len": 3,
                    "nullable": "True"
                },
                "paddingModel": {
                    "type": "string",
                    "default": "constant",
                    "enums": "constant, edge, reflect, symmetric"
                }
            },
            "gaussianBlur": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "kernelSize": 
                {
                    "type": "list[int]",
                    "default": [3, 3],
                    "min": 1,
                    "len": 2
                },
                "sigma": {
                    "type": "float",
                    "default": 0,
                    "min": 0
                }
            },
            "brightness": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "brightness": {
                    "type": "float",
                    "default": 1,
                    "min": 0
                }
            },
            "contrast": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "contrast": {
                    "type": "float",
                    "default": 1,
                    "min": 0
                }
            },
            "saturation": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "saturation": {
                    "type": "float",
                    "default": 1,
                    "min": 0
                }
            },
            "hue": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "hue": {
                    "type": "float",
                    "default": 0,
                    "max": 0.5,
                    "min": -0.5
                }
            }
        }
    },
    "ConfigPytorchModel": {
        "SelectedModel": {
            "model": {
                "structure": {
                    "type": "string",
                    "default": "auo_unrestricted_powerful_model",
                    "enums": "auo_tiny_focus_model, auo_classic_deep_model, auo_anti_degeneration_mode, auo_transform_aggregate_model, auo_narrow_intensive_model, auo_feature_enhance_model_a, auo_feature_enhance_model_b, auo_feature_enhance_model_c, auo_faster_convergency_mode, auo_unrestricted_powerful_model, auo_feature_reuse_model, auo_lighten_efficient_model, auo_horizontal_sample_model"
                    }
                },
                "pretrained": {
                    "type": "boolean",
                    "default": "True"
                }
            },
        "ClsModelPara": {
            "batchSize": {
                "type": "int",
                "default": 1,
                "min": 1
            },
            "epochs": {
                "type": "int",
                "default": 1,
                "min": 1
            }
        }
    },
    "ConfigResultStorage": {
        "ResultStorage": {
            "saveFinalWeight": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                }
            },
            "saveAccTxt": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                }
            },
            "savePredictResult": {
                "switch": {
                    "type": "boolean",
                    "default": "True"
                }
            },
            "unknownFilter": {
                "switch": {
                    "type": "boolean",
                    "default": "False"
                },
                "filter": {
                    "type": "dict[str, float]",
                    "default": {"unknown": 0.5},
                    "max": 1,
                    "min": 0
                },
                "reverse": {
                    "type": "boolean",
                    "default": "False"
                },
                "saveCsv": {
                    "type": "int",
                    "default": 2,
                    "enums": "0, 1, 2"
                }
            }
        }
    },
    "ConfigPass":{
        "confidenceFilter": "False",
        "showRate": "False",
        "cudaDevice": 0,
        "saveAccJson": "True",
        "testAccJson": "True",
        "drawAccCurve": "False",
        "drawConfusionMatrix": "True"
    }
}